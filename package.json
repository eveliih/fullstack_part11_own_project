{
  "name": "puhelinluettelon_backend",
  "version": "1.0.0",
  "description": "backend for phonebook-app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-prod": "NODE_ENV=production node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "build:ui": "rd /s/q build && cd ./front-end/ && npm run build && xcopy /I/E build ..\\build",
    "build:ubuntu": "rm -rf build && cd ./front-end/ && npm install && npm run build && cp -r build ../build",
    "eslint": "eslint './**/*.{js,jsx}'",
    "deploy": "fly deploy",
    "deploy:full": "npm run build:ui && npm run deploy",
    "cypress:open": "cypress open",
    "start:test": "SET NODE_ENV=test & node index.js",
    "test:e2e": "cypress run"
  },
  "author": "Eveliina",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.7.1",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "cypress": "^13.3.2",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^23.11.0",
    "eslint-plugin-react": "^7.19.0",
    "nodemon": "^2.0.20"
  }
}
